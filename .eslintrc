{
  "extends": "@antfu",
  "overrides": [
    {
        "files": ["scripts/**"],
        "rules": {
            "n/prefer-global/process": "off"
        }
    },
    {
      "files": ["docs/front-base/**/**"],
      "rules": {
        "no-extend-native": "off",
        "prefer-promise-reject-errors": "off",
        "no-multi-spaces": "off"
      }
    }
  ],
  "rules": {
    // yml
    "yml/no-empty-mapping-value": "off"
  }
}
